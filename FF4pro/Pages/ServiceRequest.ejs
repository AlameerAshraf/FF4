    <div class="container col-sm-12">
        <div class="panel panel-default">
            <div class="panel-heading">Application Data</div>
            <div class="panel-body">
                <div class="col-sm-6">
                    <div class="col-sm-4 ">
                        <label>Name:</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="col-sm-6 ">
                    <div class="col-sm-4 ">
                        <label>E-mail:</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" class="form-control">
                    </div>
                </div>

                <br>
                <br>


                <div class="col-sm-6">
                    <div class="col-sm-4 ">
                        <label>Phone 1:</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="col-sm-6 ">
                    <div class="col-sm-4 ">
                        <label>Phone 2:</label>
                    </div>
                    <div class="col-sm-8">
                        <input type="text" class="form-control">
                    </div>
                </div>


                <br>
                <br>

                <div class="col-sm-6">
                    <div class="col-sm-4 ">
                        <label>Address:</label>
                    </div>
